//<![CDATA[
$(function () {
	$("#tabslect li:first").addClass("selected");
	$("#tabtitle > div").hide();
	$("#tabtitle > div:first").show();
	$("#tabslect a").click(function () {
		$("#tabslect li").removeClass("selected");
		$(this).parent().addClass("selected");
		var a = $(this).attr("href");
		$("#tabtitle > div:visible").hide();
		$(a).fadeToggle(1000);
		return false
	})
});
//]]>
